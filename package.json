{
  "name": "@js-bits/fetch",
  "version": "3.1.1",
  "description": "Cross-environment (nodejs/web) fetch API",
  "keywords": [
    "javascript",
    "typescript",
    "fetch",
    "api",
    "web",
    "node",
    "browser"
  ],
  "type": "module",
  "module": "./index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./index.js"
    }
  },
  "browser": {
    "node-fetch": false,
    "abort-controller": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/js-bits/fetch"
  },
  "author": "https://github.com/apolo-gh",
  "license": "ISC",
  "engines": {
    "node": ">=14.18"
  },
  "scripts": {
    "test": "node --experimental-vm-modules --no-experimental-fetch node_modules/jest/bin/jest.js --verbose",
    "build": "rimraf ./dist && yarn build:dts",
    "build:dts": "tsc ./index.js --allowJs --emitDeclarationOnly --declaration --esModuleInterop --outDir dist",
    "test-coverage": "yarn test --coverage",
    "test-watch": "yarn test --watch",
    "lint": "tsc --noEmit && eslint '**/*.{js,jsx,ts,tsx}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.4",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@js-bits/formalinter": "^1.0.3",
    "@types/jest": "29.4.3",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "rimraf": "^5.0.1",
    "typescript": "^4.8.4"
  },
  "jest": {
    "projects": [
      {
        "displayName": "node",
        "testEnvironment": "node"
      },
      {
        "displayName": "jsdom",
        "testEnvironment": "jsdom",
        "globals": {
          "fetch": {}
        }
      }
    ]
  },
  "prettier": "@js-bits/formalinter/.prettierrc.json",
  "eslintConfig": {
    "extends": "@js-bits/eslint-config/.eslintrc.json"
  }
}
